{"ast":null,"code":"export default {\n  data() {\n    return {\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      fromVisible: false,\n      form: {},\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}')\n    };\n  },\n  created() {\n    this.load(1);\n  },\n  methods: {\n    load(pageNum) {\n      // 分页查询\n      let params = {\n        pageNum: this.pageNum,\n        pageSize: this.pageSize\n      };\n      // if (this.user.role === 'USER') {\n      //   params.userId = this.user.id\n      // }\n      // if (this.user.role === 'TEACHER') {\n      //   params.teacherId = this.user.id\n      // }\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/reserve/selectPage', {\n        params: params\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n      });\n    },\n    handleEdit(row) {\n      // 编辑数据\n      this.form = JSON.parse(JSON.stringify(row)); // 给form对象赋值  注意要深拷贝数据\n      this.fromVisible = true; // 打开弹窗\n    },\n    del(id) {\n      // 单个删除\n      this.$confirm('您确定取消吗？', '确认取消', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete('/reserve/delete/' + id).then(res => {\n          if (res.code === '200') {\n            // 表示操作成功\n            this.$message.success('操作成功');\n            this.load(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    },\n    save() {\n      this.$request.put('/reserve/update', this.form).then(res => {\n        if (res.code === '200') {\n          this.$message.success('保存成功');\n          this.fromVisible = false;\n          this.load(1);\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    checkStatus(row, status) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.form.status = status;\n      this.save();\n    }\n  }\n};","map":{"version":3,"names":["data","tableData","pageNum","pageSize","total","fromVisible","form","user","JSON","parse","localStorage","getItem","created","load","methods","params","$request","get","then","res","list","handleEdit","row","stringify","del","id","$confirm","type","response","delete","code","$message","success","error","msg","catch","save","put","handleCurrentChange","checkStatus","status"],"sources":["src/views/front/Reserve.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div style=\"width: 70%; margin: 40px auto\">\r\n      <div class=\"table\">\r\n        <el-table :data=\"tableData\" stripe>\r\n          <el-table-column prop=\"id\" label=\"序号\" width=\"80\" align=\"center\" sortable></el-table-column>\r\n          <el-table-column prop=\"userName\" label=\"家长姓名\" show-overflow-tooltip></el-table-column>\r\n          <el-table-column prop=\"userPhone\" label=\"家长电话\" show-overflow-tooltip></el-table-column>\r\n          <el-table-column prop=\"teacherName\" label=\"教师姓名\"></el-table-column>\r\n          <el-table-column prop=\"start\" label=\"开始时间\"></el-table-column>\r\n          <el-table-column prop=\"end\" label=\"结束时间\"></el-table-column>\r\n          <el-table-column prop=\"content\" label=\"预约说明\"></el-table-column>\r\n          <el-table-column prop=\"status\" label=\"预约状态\"></el-table-column>\r\n\r\n          <el-table-column label=\"操作\" width=\"180\" align=\"center\">\r\n            <template v-slot=\"scope\">\r\n              <el-button plain type=\"primary\" @click=\"handleEdit(scope.row)\" size=\"mini\" v-if=\"user.role === 'USER'\" :disabled=\"scope.row.status !== '待确认'\">编辑</el-button>\r\n              <el-button plain type=\"danger\" size=\"mini\" @click=del(scope.row.id) v-if=\"user.role === 'USER'\" :disabled=\"scope.row.status !== '待确认'\">取消预约</el-button>\r\n              <el-button plain type=\"primary\" @click=\"checkStatus(scope.row, '已接受')\" size=\"mini\" v-if=\"user.role === 'TEACHER'\" :disabled=\"scope.row.status !== '待确认'\">接受</el-button>\r\n              <el-button plain type=\"primary\" @click=\"checkStatus(scope.row, '已拒绝')\" size=\"mini\" v-if=\"user.role === 'TEACHER'\" :disabled=\"scope.row.status !== '待确认'\">拒绝</el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n\r\n        <div class=\"pagination\" style=\"margin-top: 20px\">\r\n          <el-pagination\r\n              background\r\n              @current-change=\"handleCurrentChange\"\r\n              :current-page=\"pageNum\"\r\n              :page-sizes=\"[5, 10, 20]\"\r\n              :page-size=\"pageSize\"\r\n              layout=\"total, prev, pager, next\"\r\n              :total=\"total\">\r\n          </el-pagination>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <el-dialog title=\"预约信息\" :visible.sync=\"fromVisible\" width=\"40%\" :close-on-click-modal=\"false\" destroy-on-close>\r\n      <el-form label-width=\"100px\" style=\"padding-right: 50px\" :model=\"form\">\r\n        <el-form-item prop=\"start\" label=\"开始时间\">\r\n          <el-date-picker style=\"width: 100%\"\r\n                          v-model=\"form.start\"\r\n                          type=\"date\"\r\n                          value-format=\"yyyy-MM-dd\"\r\n                          placeholder=\"选择日期\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item prop=\"end\" label=\"结束时间\">\r\n          <el-date-picker style=\"width: 100%\"\r\n                          v-model=\"form.end\"\r\n                          type=\"date\"\r\n                          value-format=\"yyyy-MM-dd\"\r\n                          placeholder=\"选择日期\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item prop=\"content\" label=\"预约说明\">\r\n          <el-input type=\"textarea\" :rows=\"5\" v-model=\"form.content\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">提 交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      fromVisible: false,\r\n      form: {},\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n    }\r\n  },\r\n  created() {\r\n    this.load(1)\r\n  },\r\n  methods: {\r\n    load(pageNum) {  // 分页查询\r\n      let params = {\r\n        pageNum: this.pageNum,\r\n        pageSize: this.pageSize,\r\n      }\r\n      // if (this.user.role === 'USER') {\r\n      //   params.userId = this.user.id\r\n      // }\r\n      // if (this.user.role === 'TEACHER') {\r\n      //   params.teacherId = this.user.id\r\n      // }\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/reserve/selectPage', {\r\n        params: params\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    handleEdit(row) {   // 编辑数据\r\n      this.form = JSON.parse(JSON.stringify(row))  // 给form对象赋值  注意要深拷贝数据\r\n      this.fromVisible = true   // 打开弹窗\r\n    },\r\n    del(id) {   // 单个删除\r\n      this.$confirm('您确定取消吗？', '确认取消', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/reserve/delete/' + id).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    save() {\r\n      this.$request.put('/reserve/update', this.form).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success('保存成功')\r\n          this.fromVisible = false\r\n          this.load(1)\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n    checkStatus(row, status) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.form.status = status\r\n      this.save()\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AAoEA;EACAA,KAAA;IACA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,WAAA;MACAC,IAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAAX,OAAA;MAAA;MACA,IAAAa,MAAA;QACAb,OAAA,OAAAA,OAAA;QACAC,QAAA,OAAAA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA,IAAAD,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAc,QAAA,CAAAC,GAAA;QACAF,MAAA,EAAAA;MACA,GAAAG,IAAA,CAAAC,GAAA;QACA,KAAAlB,SAAA,GAAAkB,GAAA,CAAAnB,IAAA,EAAAoB,IAAA;QACA,KAAAhB,KAAA,GAAAe,GAAA,CAAAnB,IAAA,EAAAI,KAAA;MACA;IACA;IACAiB,WAAAC,GAAA;MAAA;MACA,KAAAhB,IAAA,GAAAE,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAe,SAAA,CAAAD,GAAA;MACA,KAAAjB,WAAA;IACA;IACAmB,IAAAC,EAAA;MAAA;MACA,KAAAC,QAAA;QAAAC,IAAA;MAAA,GAAAT,IAAA,CAAAU,QAAA;QACA,KAAAZ,QAAA,CAAAa,MAAA,sBAAAJ,EAAA,EAAAP,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAW,IAAA;YAAA;YACA,KAAAC,QAAA,CAAAC,OAAA;YACA,KAAAnB,IAAA;UACA;YACA,KAAAkB,QAAA,CAAAE,KAAA,CAAAd,GAAA,CAAAe,GAAA;UACA;QACA;MACA,GAAAC,KAAA,QACA;IACA;IACAC,KAAA;MACA,KAAApB,QAAA,CAAAqB,GAAA,yBAAA/B,IAAA,EAAAY,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAW,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAA3B,WAAA;UACA,KAAAQ,IAAA;QACA;UACA,KAAAkB,QAAA,CAAAE,KAAA,CAAAd,GAAA,CAAAe,GAAA;QACA;MACA;IACA;IACAI,oBAAApC,OAAA;MACA,KAAAW,IAAA,CAAAX,OAAA;IACA;IACAqC,YAAAjB,GAAA,EAAAkB,MAAA;MACA,KAAAlC,IAAA,GAAAE,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAe,SAAA,CAAAD,GAAA;MACA,KAAAhB,IAAA,CAAAkC,MAAA,GAAAA,MAAA;MACA,KAAAJ,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"modul